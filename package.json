{
  "name": "typescriptwebpackmochatemplate",
  "version": "1.0.0",
  "description": "minimal typescript setup with webpack dev server and mocha testrunner",
  "scripts": {
    "build": "node ./node_modules/webpack/bin/webpack --config webpack.config.dev.js",
    "start": "webpack-dev-server --config webpack.config.dev.js --open",
    "test": "test"
  },
  "dependencies": {
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "@types/webpack": "3.8.2",
    "css-loader": "^0.28.8",
    "html-webpack-plugin": "^2.30.1",
    "nsp": "^3.1.0",
    "style-loader": "^0.19.1",
    "ts-loader": "3.2.0",
    "typescript": "2.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.10.0"
  },
  "keywords": [
    "typescript",
    "mocha",
    "webpack"
  ],
  "author": "Johannes Lindgren",
  "license": "GPL-3.0"
}
