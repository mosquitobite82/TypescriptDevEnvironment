{
  "name": "typescriptwebpackmochatemplate",
  "version": "1.0.0",
  "description": "minimal typescript setup with webpack dev server and mocha & chai test setup",
  "scripts": {
    "build": "node ./node_modules/webpack/bin/webpack --config webpack.config.dev.js",
    "start": "webpack-dev-server --config webpack.config.dev.js --open",
    "test": "mocha -r ts-node/register -r ignore-styles test/**/*.spec.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "4.1.0",
    "@types/mocha": "2.2.46",
    "@types/node": "9.3.0",
    "@types/webpack": "3.8.2",
    "chai": "4.1.2",
    "css-loader": "0.28.8",
    "html-webpack-plugin": "2.30.1",
    "ignore-styles": "5.0.1",
    "mocha": "4.1.0",
    "node-sass": "4.7.2",
    "nsp": "^3.1.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "ts-loader": "3.2.0",
    "ts-node": "4.1.0",
    "typescript": "2.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.10.0"
  },
  "keywords": [
    "typescript",
    "mocha",
    "webpack"
  ],
  "author": "Johannes Lindgren",
  "license": "GPL-3.0"
}
